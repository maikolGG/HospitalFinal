<h1>CENTRO MEDICO BACKEND</h1>
<h2>LISTADO DE MEDICOS</h2>
<table>
<thead>
    <tr>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Cedula</th>
        <th>Especialidad</th>
        <th>Consultorio</th>
        <th>Correo</th>
        <th>Opciones</th>
    </tr>
</thead>
<tbody>
    {{#each medicos}}
    <tr>
        <td>{{nombres}}</td>
        <td>{{apellidos}}</td>
        <td>{{cedula}}</td>
        <td>{{especialidad}}</td>
        <td>{{consultorio}}</td>
        <td>{{correo}}</td>
        <td><a href="/medicos/eliminar/{{cedula}}">Eliminar</a></td>
    </tr>
    {{/each}}
</tbody>
</table>
<a class="volver" href="/">Volver</a>
<a href="medicos/agregar-medico">Agregar doctor</a>